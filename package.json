{
  "name": "typescript-starter",
  "version": "1.0.0",
  "description": "A basic typescript app starter for newbies in 2022.",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "dev": "nodemon",
    "start": "npm run build && node build/index.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js  -d ./src/configs/ORM",
    "migration:up": "npm run typeorm  migration:run",
    "migration:down": "npm run typeorm  migration:revert",
    "migration:generate": "npm run typeorm -- migration:generate ./src/infras/data/migrations/%npm_config_name%",
    "migration:create": "npm run typeorm migration:create"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run prettier-format && npm run lint"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "chai": "^4.3.6",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "run-script-os": "^1.1.6",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "@types/mime-types": "^2.1.1",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.6",
    "@types/paypal-rest-sdk": "^1.7.6",
    "chalk": "^5.1.0",
    "class-transformer": "^0.3.1",
    "class-transformer3": "npm:class-transformer@^0.3.1",
    "class-validator": "^0.13.2",
    "class-validator-jsonschema": "^3.1.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.35",
    "minio": "^7.0.32",
    "module-alias": "^2.2.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.8.0",
    "openapi3-ts": "^3.0.2",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "paypal-rest-sdk": "^1.8.1",
    "pg": "^8.8.0",
    "redis": "^4.3.1",
    "redis-commands": "^1.7.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "socket.io": "^4.5.2",
    "swagger-ui-express": "^4.5.0",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.4.0",
    "typedi": "^0.10.0",
